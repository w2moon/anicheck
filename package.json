		"lint": "prettier --check .",
		"build:pages": "npm run build && echo 'Build completed for GitHub Pages'"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
