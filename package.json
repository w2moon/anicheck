		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@esotericsoftware/spine-pixi-v8": "~4.2.0",
		"@pixi/ui": "^2.2.7",
		"pixi.js": "^8.13.2"
	}
}
